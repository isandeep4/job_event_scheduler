    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "axios": "1.3.4",
    "eslint": "^8.35.0",
    "jest": "29.4.3",
    "jest-mock-extended": "3.0.1",
    "ts-jest": "29.0.5",
    "ts-node": "10.4.0",
